{
  "name": "nyx",
  "version": "1.0.0-b.1",
  "description": "Combines the best of Python and JavaScript while remaining fast, stable, resource efficient and reliable. Allowing users to build anything.",
  "bin": {
    "nyx": "dist/cli.js"
  },
  "author": {
    "name": "Jonas F. Franke",
    "email": "me@binaryblazer.me",
    "url": "https://binaryblazer.me"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --exec \"tsc\" --ext \"ts\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "bun scripts/postinstall.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nyx",
    "python",
    "javascript",
    "typescript",
    "fast",
    "stable",
    "resource efficient",
    "reliable"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bun": "^1.1.17",
    "@types/node": "^22.10.7",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "node-fetch": "^3.3.2"
  }
}
